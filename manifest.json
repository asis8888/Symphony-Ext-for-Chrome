{
  "manifest_version": 2,
  "name": "Symphony Update Extension",
  "version": "1.3",
  "permissions": [
      "storage",
      "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.linkedin.com/*"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "beasts-32.png",
    "default_popup": "popup_page.html"
  }
}